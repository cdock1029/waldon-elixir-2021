<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "property-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :name %>
  <%= textarea f, :name %>
  <%= error_tag f, :name %>

  <%= label f, :address %>
  <%= textarea f, :address %>
  <%= error_tag f, :address %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
